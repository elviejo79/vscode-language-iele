{
    "name": "language-iele",
    "displayName": "Language IELE",
    "description": "Syntax highlighting for IELE",
    "version": "0.0.4",
    "publisher": "shd101wyy",
    "repository": "https://github.com/shd101wyy/vscode-language-iele",
    "license": "MIT",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "iele",
                "aliases": [
                    "IELE",
                    "iele"
                ],
                "extensions": [
                    ".iele"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "iele",
                "scopeName": "source.iele",
                "path": "./syntaxes/iele.tmLanguage.json"
            }
        ]
    }
}